<div class="oslc-selection-dialog">
  <div class="hidden" data-response="<%= @response %>"></div>

  <%= form_tag request.original_url, class:'search-form', :method => :get do %>
    <div class="input-group">
      <%= text_field_tag :search, params[:search], :class => "search-query form-control", :placeholder => t(:search_test_plans, scope: :oslc) %>
      <span class="input-group-btn">
        <button class="btn btn-default" type="submit"><%= icon('search') %></button>
      </span>
    </div>
  <% end %>

  <%= render partial: "/tmt/test_runs/table",
    locals: {
      project: @service_provider,
      test_runs: @test_plans,
      title: t(:listing_test_case, scope: :projects)
    }
  %>

</div>
