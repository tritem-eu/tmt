<%= left_navigation_for_admin active: :users %>

<%= page_header t(:user, scope: :users),
  subject: h(@user.name)
%>

<%= breadcrumbs(
  :admin,
  admin_path,
  t(:users, scope: :users),
  admin_users_path,
  h(@user.name),
  user_path(@user),
  :edit
) %>

<%= render 'form',
  url: admin_user_path(@user),
  user: @user
%>
